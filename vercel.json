{
    "version": 2,
    "builds": [
      {
        "src": "django_ml/wsgi.py", 
        "use": "@vercel/python",
        "config":{ "maxLambdaSize": "240mb","runtime":"python3.9"}
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "django_ml/wsgi.py"
      }
    ],
    "functions": {
        "api/test.js": {
          "memory": 3008,
          "maxDuration": 30 
        },
        "api/*.js": {
          "memory": 3008,
          "maxDuration": 15 
        }
      }


}

  