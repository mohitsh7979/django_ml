{
    "version": 2,
    "builds": [
        {
            "src": "django_ml/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "250mb",
                "runtime": "python3.9"
            },
            "exclude": ["large-assets", "unnecessary-folder"]
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "django_ml/wsgi.py"
        }
    ]
}